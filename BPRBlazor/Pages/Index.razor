@page "/"
@inject IHttpService HttpService
@inject IDependencyComponentService DependencyComponentService
@inject ICodebaseService CodebaseService
@using BPRBE.Services
@implements IDisposable

<button class="btn btn-primary" @onclick="SendDataAsync">Send Data</button>
<DownloadButton />
<InputFile OnChange="LoadCodeSource" accept=".7z"/>
<p>@_errorMessage</p>

<div>
<tbody>
    @foreach (var item in _folders)
    {
        <tr>
            <td>@item</td>
        </tr>
    }
</tbody>
</div>